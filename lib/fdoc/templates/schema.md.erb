<%= 'Deprecated' if deprecated? -%>
<%= @schema["description"] %>
  <%= "* __Required__: #{required?}" if nested? %>
  <%= "* __Type__: #{type}" if type %>
  <%= "* __Format__: #{format}" if format %>
  <%= "* __Example__: #{example}" if example %>
<%= @schema["enum"] %>
<% (@schema.keys - Fdoc::SchemaPresenter::FORMATTED_KEYS).each do |key| %>
<%= "* %{key} %{@schema[key]}" %>
<% end %>
<% if items = @schema["items"] %>
* Items
  <% if items.kind_of? Array %>
  <% item.compact.each do |item| %>
  <%= Fdoc::SchemaPresenter.new(item, options.merge(nested: true)).to_markdown %>
  <% end %>
  <% else %>
  <%= Fdoc::SchemaPresenter.new(@schema["items"], options.merge(nested: true)).to_markdown %>
  <% end %>
<% end %>
<% if properties = @schema["properties"] %>
<% properties.each do |key, property| %>
<% next if property.nil? %>
<%= "* __#{key}__:" %>
  <%= Fdoc::SchemaPresenter.new(property, options.merge(nested: true)).to_markdown %>
<% end %>
<% end %>
